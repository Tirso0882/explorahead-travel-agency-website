    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@sentry/nextjs": "^10.34.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.25",
